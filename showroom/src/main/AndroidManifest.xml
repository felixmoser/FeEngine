<manifest
	package="de.fieben.feengine.showroom"
	xmlns:android="http://schemas.android.com/apk/res/android">

	<application
		android:allowBackup="true"
		android:icon="@drawable/feengineshowroom_launcher"
		android:label="@string/app_name"
		android:theme="@style/AppTheme">
		<activity android:name=".activities.SceneListActivity">
			<intent-filter android:label="@string/app_name">
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity
			android:name=".activities.SceneDetailActivity"
			android:screenOrientation="portrait">
			<meta-data
				android:name="android.support.PARENT_ACTIVITY"
				android:value=".activities.SceneListActivity" />
		</activity>
	</application>
</manifest>
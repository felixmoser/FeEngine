<manifest
	xmlns:android="http://schemas.android.com/apk/res/android"
	package="de.fieben.fengine.showroom"
	android:versionCode="1"
	android:versionName="1.0">
	<uses-sdk
		android:minSdkVersion="14"
		android:targetSdkVersion="17"/>
	<application
		android:icon="@drawable/ic_launcher"
		android:theme="@style/AppTheme"
		android:label="@string/app_name"
		android:allowBackup="true">
		<activity
			android:name=".SceneListActivity"
			android:label="@string/title_scene_list">
			<intent-filter android:label="@string/app_name">
				<action android:name="android.intent.action.MAIN"/>
				<category android:name="android.intent.category.LAUNCHER"/>
			</intent-filter>
		</activity>
		<activity
			android:name=".SceneDetailActivity"
			android:label="@string/title_scene_detail">
			<meta-data
				android:name="android.support.PARENT_ACTIVITY"
				android:value=".SceneListActivity"/>
		</activity>
	</application>
</manifest>